{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "max-lines": [
      "warn",
      { "max": 450, "skipComments": true, "skipBlankLines": true }
    ],
    "no-unused-vars": "warn",
    "func-names": "warn",
    "spaced-comment": "error",
    "consistent-return": "warn",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
